<?xml version="1.0"?>
<launch>
  <rosparam command="load" file="$(find diagnostic_updater)/src/updaters_params.yaml" />

  <node pkg="diagnostic_updater" type="imu_updater_node" name="imu_updater">
    <param name="diagnostic_period" value="0.05"/>
  </node>

  <node pkg="diagnostic_updater" type="jointstates_updater_node" name="jointstates_updater">
    <param name="diagnostic_period" value="0.05"/>
  </node>
  
  <node pkg="diagnostic_updater" type="navcam_updater_node" name="navcam_updater">
    <param name="diagnostic_period" value="0.05"/>
  </node>

  <node pkg="diagnostic_updater" type="telecam_updater_node" name="telecam_updater">
    <param name="diagnostic_period" value="0.05"/>
  </node>

</launch>

